{
    "manifest_version": 3,
    "name": "Tab Tracker",
    "version": "1.0",
    "description": "Tracks your open tabs. Created by Bathula Koushik Yadav.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
  